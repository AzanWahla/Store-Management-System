<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ItemScreen">
   <children>
      <Label alignment="CENTER" layoutX="134.0" layoutY="31.0" prefHeight="60.0" prefWidth="301.0" text="Item Section">
         <font>
            <Font name="Baskerville Old Face" size="35.0" />
         </font>
      </Label>
      <Button fx:id="addItem" layoutX="108.0" layoutY="133.0" mnemonicParsing="false" onAction="#addItemScreen" prefHeight="35.0" prefWidth="101.0" text="Add Item" />
      <Button fx:id="back" layoutX="183.0" layoutY="330.0" mnemonicParsing="false" onAction="#backScreen" text="Back" />
      <Button fx:id="displayItem" layoutX="362.0" layoutY="133.0" mnemonicParsing="false" onAction="#displayItemScreen" prefHeight="35.0" prefWidth="101.0" text="Display Item" />
      <Button fx:id="updateItem" layoutX="362.0" layoutY="249.0" mnemonicParsing="false" onAction="#updateItemScreen" prefHeight="35.0" prefWidth="101.0" text="Update Item" />
      <Button fx:id="deleteItem" layoutX="108.0" layoutY="249.0" mnemonicParsing="false" onAction="#deleteItemScreen" prefHeight="35.0" prefWidth="101.0" text="Delete Item" />
      <Button fx:id="close" layoutX="336.0" layoutY="330.0" mnemonicParsing="false" onAction="#close" text="Close" />
   </children>
</AnchorPane>
