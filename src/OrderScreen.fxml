<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="OrderScreen">
   <children>
      <Label fx:id="title" alignment="CENTER" layoutX="180.0" layoutY="29.0" prefHeight="63.0" prefWidth="215.0" text="Order Section">
         <font>
            <Font name="Baskerville Old Face" size="35.0" />
         </font>
      </Label>
      <Button fx:id="addOrder" layoutX="96.0" layoutY="141.0" mnemonicParsing="false" onAction="#addOrderScreen" prefHeight="95.0" prefWidth="105.0" text="Add Order" />
      <Button fx:id="back" layoutX="180.0" layoutY="313.0" mnemonicParsing="false" onAction="#backScreen" text="Back" />
      <Button fx:id="viewOrder" layoutX="343.0" layoutY="141.0" mnemonicParsing="false" onAction="#viewOrderScreen" prefHeight="95.0" prefWidth="105.0" text="View Order" />
      <Button fx:id="close" layoutX="317.0" layoutY="313.0" mnemonicParsing="false" onAction="#close" text="Close" />
   </children>
</AnchorPane>
