<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="DisplayEmployeeScreen">
   <children>
      <Label fx:id="title" alignment="CENTER" layoutX="141.0" layoutY="63.0" prefHeight="62.0" prefWidth="375.0" text="Display Employee Section">
         <font>
            <Font name="Baskerville Old Face" size="35.0" />
         </font>
      </Label>
      <TableView fx:id="table" layoutX="19.0" layoutY="131.0" prefHeight="200.0" prefWidth="573.0">
         <columns>
            <TableColumn fx:id="ColID" prefWidth="85.0" text="Employee ID" />
            <TableColumn fx:id="ColName" minWidth="5.0" prefWidth="110.0" text="Employee Name" />
            <TableColumn fx:id="ColAddress" prefWidth="122.0" text="Employee Address" />
            <TableColumn fx:id="ColPhone" prefWidth="121.0" text="Employee Phone" />
            <TableColumn fx:id="ColPosition" prefWidth="154.0" text="Employee Position" />
         </columns>
      </TableView>
      <Button fx:id="back" layoutX="175.0" layoutY="357.0" mnemonicParsing="false" onAction="#backScreen" prefHeight="31.0" prefWidth="76.0" text="Back" />
      <Button fx:id="close" layoutX="383.0" layoutY="357.0" mnemonicParsing="false" onAction="#closeScreen" prefHeight="31.0" prefWidth="92.0" text="Close" />
   </children>
</AnchorPane>
